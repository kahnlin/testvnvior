    function initMap() {
        var hkAddress = { lat: 22.278208, lng: 114.182299 };
        var map = new google.maps.Map(document.getElementById('map'), {
            zoom: 17,
            center: hkAddress
        });
        var markerHk = new google.maps.Marker({
            position: hkAddress,
            map: map
        });


        var twAddress = { lat: 25.080638, lng: 121.564503 };
